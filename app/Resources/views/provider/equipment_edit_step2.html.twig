{% extends 'base.html.twig' %}

{% block home_header %}{% endblock %}

{% block body %}
    {% include('provider/user_nav.html.twig') %}
    <section class="users container background-light-grey">
      <div class="row">
          <!-- Left side -->
          <div class="col-md-9">
              <img src="/img/logo-small.svg" alt=""/>            <ul class="steps">
                  <li class="step-1 active-step"><span class="counters"><span class="n-one"></span>Tarif</span></li>
                  <li class="step-2"><span class="counters text-bold"><span class="n-two"></span>Beschreibung</span>
                  </li>
                  <li class="step-3"><span class="counters greyed"><span class="nh-three"></span>Details</span></li>
                  <li class="step-4"><span class="counters greyed"><span class="nh-four"></span>Fertig</span></li>
              </ul>
          </div>
          <!-- Right side -->
          <div class="col-md-3 margin-rwd">
            <div class="support">
              <p class="head">Anbieter Support</p>
              <p class="mail"><a href="mail:support@hey-tsc.com">support@hey-tsc.com</a></p>
            </div>
          </div>
      </div>
      <div class="row user-forms">
          <div class="col-md-8">
              <div class="row">
                  <div class="col-xs-12 border-bottom-white form-head">
                      <div class="form-container form-input-border">
                          <div class="row">
                              <div class="col-sm-12">
                                  <span class="number-two number-rwd"></span>

                                  <p class="form-heading">Gib deinem Angebot ein Gesicht!</p>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
              <div class="upload-form">
                  <form id="upload-once" method="post" action="{{ path('equipment-image') }}" enctype="multipart/form-data">
                      <span class="btn btn-default btn-file">
                         bild hochladen <input type="file">
                       </span>
                  </form>
                  <p class="text-center what-size">Bildgröße bitte mindestens 1024 x 768px im Querformat</p>
                  <form id="upload" method="post" action="{{ path('equipment-image') }}" enctype="multipart/form-data">
                      <div  id="drop">
                          <img class="display-block" src="data:image/png;base64,"/>
                          <a>Bilder hochladen*</a>
                          <input type="file" name="upl" multiple/>
                      </div>
                      <ul>
                          <!-- The file uploads will be shown here -->
                      </ul>
                  </form>
                  <p class="how-many">Du kannst 3 Bilder hochladen (maximal 10MB, mindestens 1024 x 768px). Unterstützte Bildformate: jpg, png</p>
                  <span class="tutorial">tutorial</span>
              </div>
              {{ form_start(form) }}
              <div class="row">
                  <div class="col-xs-12 border-bottom-white form-head describe">
                      <div class="form-container border-bottom-white">
                          <div class="row">
                              <div class="col-sm-12">
                                  <p class="form-heading-small margin-none">Beschreibe dein Angebot*</p>
                                  <div class="err-msg">{{ form_errors(form) }}</div>
                              </div>
                          </div>
                      </div>
                      <div class="form-container form-input-border padding-30">
                          <div class="row">
                              <div class="col-sm-12">
                                  {# <textarea placeholder="Maximal 500 Zeichen verfügbar"></textarea> #}
                                  {{ form_widget(form.description, { 'attr': { 'placeholder': 'Maximal 500 Zeichen verfügbar' } }) }}
                                  <div class="err-msg">{{ form_errors(form.description) }}</div>
                                  <div class="chbox-holder">
                                      {# <input type="checkbox" id="c2hx"> #}
                                      {{ form_widget(form.make_sure) }}
                                      <label for="form_make_sure"><span></span>Bitte versichere, dass dein Equipment zum Zeitpunkt der Buchung funktionstüchtig und
      einsatzbereit sein wird.</label>
                                  </div>
                                  <div class="err-msg">{{ form_errors(form.make_sure) }}</div>
                              </div>
                          </div>
                      </div>
                      <div class="form-container border-bottom-white margin-ort">
                          <div class="row ort">
                              <div class="col-sm-12">
                                  <p class="form-heading-small margin-none">Übergabeort</p>
                              </div>
                          </div>
                      </div>
                      <div class="form-container form-input-border padding-30">
                          <div class="row ort">
                              <div class="col-sm-12">
                                  <div class="row">
                                    <div class="col-xs-8">
                                      {# <input type="text" placeholder="Strasse*" /> #}
                                        {{ form_widget(form.street, { 'attr': { 'placeholder': 'Strasse' } }) }}
                                        <div class="err-msg">{{ form_errors(form.street, { 'attr': { 'class': 'err-msg' } }) }}</div>
                                    </div>
                                    <div class="col-xs-4">
                                      {# <input type="text" placeholder="Hausnr.*" /> #}
                                        {{ form_widget(form.number, { 'attr': { 'placeholder': 'Hausnr.' } }) }}
                                        <div class="err-msg">{{ form_errors(form.number, { 'attr': { 'class': 'err-msg' } }) }}</div>
                                    </div>
                                  </div>
                                  <div class="row">
                                    <div class="col-xs-3">
                                      {# <input type="text" placeholder="Plz*" /> #}
                                        {{ form_widget(form.postcode, { 'attr': { 'placeholder': 'Plz' } }) }}
                                        <div class="err-msg">{{ form_errors(form.postcode, { 'attr': { 'class': 'err-msg' } }) }}</div>
                                    </div>
                                    <div class="col-xs-5">
                                      {# <input type="text" placeholder="Ort*" /> #}
                                        {{ form_widget(form.place, { 'attr': { 'placeholder': 'Ort' } }) }}
                                        <div class="err-msg">{{ form_errors(form.place, { 'attr': { 'class': 'err-msg' } }) }}</div>
                                    </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                      <div class="chbox-holder alone-chbox">
                          {# <input type="checkbox" id="c2hxx"> #}
                          {{ form_widget(form.accept) }}
                          <label for="form_accept"><span></span>Ich akzeptiere die <a href="#" data-toggle="modal" data-target=".allgemeine">Allgemeinen Geschäftsbedingungen</a></label>
                      </div>
                      <div class="err-msg">{{ form_errors(form.accept) }}</div>
                      <input type="submit" class="yellow-button user-submit" value="Einstellungen speichern">
                  </div>
              </div>
            {{ form_end(form) }}
          </div>
          <div class="col-md-4 margin-rwd">
              <div class="support portfolios">
                  <p class="head">Deine Angebotsfotos</p>
                  <p class="text-bold">Ein tolles Foto zu deiner Anzeige ver-
                      bessert die Chancen, dass dein Angebot
                      gebucht wird um ein Vielfaches!</p>
                  <p>Hast du ein tolles Produkt, benötigst
                      für deine Anzeige aber Hilfe, mit denProduktfotos? Dann kannst du unseren
                      <a href="#"> Photoservice</a> in Anspruch nehmen. Schreib uns dazu an <a href="mailto:support@hey-tsc.com">support@hey-tsc.com</a> </p>
                  <div id="owl-demo" class="owl-carousel user-owl">
                      <div class="item">
                          <img src="/img/placeholder/bike5.jpg" alt=""/>                    </div>
                      <div class="item">
                          <img src="/img/placeholder/bike5.jpg" alt=""/>                    </div>
                      <div class="item">
                          <img src="/img/placeholder/bike5.jpg" alt=""/>                    </div>
                  </div>
              </div>
          </div>
          <!--<div class="col-xs-4">
              <div class="support portfolios videos">
                  <p class="head">Angebotsvideo? </p>
                  <p class="text-bold">Hast du für dein Produkt bereits
                      ein Video?</p>
                  <p>Wir bieten einen einzigartigen Video-
                      service für unsere Talente. Schreib bitte an <a href="mailto:support@hey-tsc.com">video@hey-tsc.com</a> um was für  ein
                      Angebot es sich handelt. Wir melden uns
                      die nächsten Tage mit näheren
                      Infos zu unserem Service bei dir!</p>
              </div>
          </div>-->
      </div>
  </section>
